<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotlinAndroidKnowledges"
        tools:targetApi="31" >
        <!--
             TODO: Before you run your application, you need a Google Maps API key.

             To get one, follow the directions here:

                https://developers.google.com/maps/documentation/android-sdk/get-api-key

             Once you have your API key (it starts with "AIza"), define a new property in your
             project's local.properties file (e.g. MAPS_API_KEY=Aiza...), and replace the
             "YOUR_API_KEY" string in this file with "${MAPS_API_KEY}".
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDqIBd6VcWfIxDB8ks_JSqjQ34ucIScjso" />

        <service android:name=".android.services.CustomService" />

        <receiver android:name=".android.broadcast_receiver.AirPlaneModeReceiver" />
        <receiver
            android:name=".android.broadcast_receiver.TimeZoneChangeReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.TIMEZONE_CHANGED" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".android.permissions.location.UserLocationActivity"
            android:exported="true" >

        </activity>
        <activity
            android:name=".android.permissions.location.MapsActivity"
            android:exported="true" />

        <activity
            android:name=".android.video_player.VideoPlayerActivity"
            android:exported="true" >

        </activity>
        <activity
            android:name=".android.local_db.data_store.DSActivity"
            android:exported="true" />
        <activity
            android:name=".android.work_manager.WorkManagerActivity"
            android:exported="true" />
        <activity
            android:name=".android.notification.NotificationActivity"
            android:exported="true" />
        <activity
            android:name=".android.alert_dialog.AlertDialogActivity"
            android:exported="true" />
        <activity
            android:name=".android.network.NetworkActivity"
            android:exported="true" />
        <activity
            android:name=".android.broadcast_receiver.TestBroadcastReceiverActivity"
            android:exported="true" />
        <activity
            android:name=".android.audio_player.PlayAudioActivity"
            android:exported="true" />
        <activity
            android:name=".android.permissions.contact.ContactActivity"
            android:exported="true" />
        <activity
            android:name=".android.permissions.services.CustomServiceActivity"
            android:exported="true" />
        <activity
            android:name=".android.permissions.read_external_storage.ReadExternalStorageActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>